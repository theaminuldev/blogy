{
	"version": 2,
	"builds": [
		{
			"src": "webpack.config.cjs",
			"use": "@vercel/webpack"
		}
	],
	"routes": [
		{
			"src": "/(.*\\.(?:css|js))$",
			"dest": "/$1",
			"headers": {
				"cache-control": "s-maxage=31536000,immutable"
			}
		},
		{
			"src": "/(.*\\.(?:png|jpg|jpeg|gif|ico|svg))$",
			"dest": "/$1",
			"headers": {
				"cache-control": "s-maxage=31536000,immutable"
			}
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}